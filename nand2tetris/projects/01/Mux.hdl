/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=sel, out=notsel);
	And(a=a,b=notsel,out=first);
	And(a=b,b=sel,out=sec);
	Or(a=first,b=sec,out=out);
}